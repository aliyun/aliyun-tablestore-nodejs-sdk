{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "charset": "utf8",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "importHelpers": true,
    "lib": ["esnext"]
  }
}
